{
	"name": "svkit",
	"version": "5.0.1",
	"engines": {
		"node": ">18"
	},
	"scripts": {
		"dev": "vite dev --port 6110",
		"build": "vite build &&  npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync &&  sass ./src/lib --no-source-map && svelte-package -o ./publish/dist",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"cpy": "./src/script/cpy_package-json.js",
		"publish": "cd ./publish && npm run publish"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./helper": {
			"types": "./dist/helpers/index.d.ts",
			"default": "./dist/helpers/index.js"
		},
		"./states": {
			"types": "./dist/states/index.d.ts",
			"default": "./dist/states/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^2.17.3",
		"svelte": "^5.20.5"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.6",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.0.0",
		"@tailwindcss/vite": "^4.0.9",
		"@types/node": "^22.13.8",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-svelte": "^3.0.2",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.7",
		"sass": "^1.85.1",
		"svelte-check": "^4.1.4",
		"tailwind-merge": "^3.0.2",
		"tailwindcss": "^4.0.9",
		"typescript": "~5.7.0",
		"typescript-eslint": "^8.23.0",
		"vite": "^6.2.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.2"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"esbuild"
		]
	}
}